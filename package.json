{
  "name": "@mgraphic/cipher-token",
  "description": "Package to encrypt and decrypt data into a storable string with multiple options to generate a key",
  "version": "0.0.4",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/mgraphic/cipher-token"
  },
  "scripts": {
    "e2e": "ts-node ./e2e.ts",
    "prebuild": "npm ci && npm run test:coverage && npm run e2e",
    "build": "tsup",
    "postbuild": "npm run typedoc",
    "test": "npm run test:coverage",
    "test:watch": "jest --updateSnapshot --watchAll",
    "test:coverage": "jest --updateSnapshot --coverage",
    "typedoc": "typedoc --plugin typedoc-plugin-markdown --entryPoints src --out docs"
  },
  "keywords": [
    "encryption",
    "decryption",
    "crypto"
  ],
  "author": "Keith Marshall <keith@kmarshall.com>",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.3",
    "typedoc": "^0.26.5",
    "typedoc-plugin-markdown": "^4.2.3",
    "typescript": "^5.5.4"
  }
}
